# Development Settings
ENV=development
DEBUG=true
PORT=8000

# Security Keys (Replace these with secure values)
API_SECRET_KEY=dev_key_replace_in_production
JWT_SECRET_KEY=dev_key_replace_in_production
WEBSITE_API_KEY=dev_key_replace_in_production
ADMIN_API_KEY=dev_key_replace_in_production

# API Security
API_KEY_HEADER_NAME=X-API-Key
REQUIRE_API_KEY=false

# Download Settings
DOWNLOAD_FOLDER=downloads
MAX_DOWNLOADS=10
MAX_CONCURRENT_DOWNLOADS=5
DOWNLOAD_EXPIRY_MINUTES=60
VERIFY_SSL=false

# Development URLs
FRONTEND_URL=http://localhost:3000